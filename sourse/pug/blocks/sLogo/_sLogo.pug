mixin sLogo(data)
	// start sLogo
	+b.SECTION.sLogo.section#sLogo&attributes(attributes)
		.container
			+b.section-title
				h2 Официальные дилеры
				
			+e.slider-wrapper
				+e.slider--js.swiper
					.swiper-wrapper
						- for (var x = 0; x < 6; x++)
							+e.slide.swiper-slide
								img(src="img/logo-1.png", alt="")
	// end sLogo